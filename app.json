{
  "expo": {
    "name": "Chatapp",
    "slug": "chatApp",
    "version": "1.0.0",
    "orientation": "portrait",

    "icon": "./assets/images/splashImage.png",

    "scheme": "chatapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "package": "com.swalihmc.chatapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splashImage.png",
        "backgroundColor": "#E6F4FE"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/splashImage.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Chatapp to access your photos",
          "cameraPermission": "Allow Chatapp to access your camera"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splashImage.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],

    "runtimeVersion": {
      "policy": "appVersion"
    },

    "updates": {
      "url": "https://u.expo.dev/003ff093-10c2-42bc-bdae-33304a365a23"
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "003ff093-10c2-42bc-bdae-33304a365a23"
      }
    }
  }
}
